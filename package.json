{
  "name": "alchemy-folder-view",
  "displayName": "Alchemy Folder View",
  "description": "Displays a folder hierarchy in a new tab.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "activationEvents": [
    "onCommand:alchemyFolderView.openWebview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "alchemyFolderView.openWebview",
        "title": "Alchemy: Open Folder Explorer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "typescript": "^5.1.6"
  }
}
